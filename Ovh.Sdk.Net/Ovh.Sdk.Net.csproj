<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Version>0.1.3</Version>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Full typed OVH SDK</Title>
	<Description>This package provides a fully typed SDK for OVH, offering complete type safety for both API calls and models. With this SDK, you can seamlessly interact with OVH services, ensuring clear and reliable interactions with their APIs. Ideal for developers who need to work with OVH's infrastructure while leveraging type support for better development experience and fewer runtime errors.</Description>
    <PackageId>OVHSDK</PackageId>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/bmassemin/ovh-sdk-net.git</RepositoryUrl> 
	<RepositoryType>git</RepositoryType> 
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
